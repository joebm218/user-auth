    <body>
        

      <div id="mySidenav" class="sidenav">
        <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
        <div class="pkg_img_cnt">

        </div>
        <a href="#">About</a>
        <a href="#">Services</a>
        <a href="#">Clients</a>
        <a href="#">Contact</a>
      </div>
      <div class="container">
        <div class="top_bar">
          <div class="row">
            <div class="col-md-12">

              <div class="col-md-3 col-sm-12 col-xs-12">
                <span style="line-height: 54px;font-size:20px;cursor:pointer;color: #000;" onclick="openNav()">&#9776;</span>
              </div>
              <div class="col-md-6 col-sm-12 col-xs-12 text-center">
                <img src="img/logo.png">
              </div>
              <div class="col-md-3 col-sm-12 col-xs-12 profile_det">
                <div class="pro_pic_cnt">

                </div>
                <span>John Doe</span>
              </div>
            </div>
          </div>
        </div>
        <div class="search_bar">
        <div class="search_box">        
          <div class="row">
            <form method="POST" action="/searchresult2">
            <div class="col-md-12">
              <div class="col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                  <label for="">Departure</label>
                  <input type="text" placeholder="Departure" name="departure" required="" class="form-control">
                </div>
              </div>
              <div class="col-md-3 col-sm-6 col-xs-12">
                <div class="form-group">
                  <label for="">Arrival</label>
                  <input type="text" placeholder="Arrival" name="arrival" required="" class="form-control">
                </div>
              </div>
              <div class="col-md-4 col-sm-6 col-xs-12">
                <div class="col-md-6 col-sm-12 col-xs-12">
                  <div class="form-group">
                    <label for="">Date Range From</label>
                    <input type="date" placeholder="From" name="date"  class="form-control">
                  </div>
                </div>
                <div class="col-md-6 col-sm-12 col-xs-12">
                  <div class="form-group">
                    <!-- <label for="">Round Trip</label> -->
                    <select class="form-control rng_to" name="roundtrip"  >
                      <option value="roundtrip">Round Trip</option>
                      <option value="oneside">One Side</option>
                    </select>
                  </div>
                </div>
              </div>
               <div class="col-md-2 col-sm-6 col-xs-12">
                   <button type="submit" class="btn btn-md search_flights btn_bg_black">Search Flights</button> 
               </div>
            </div>
            </form>
          </div>
        </div>
        </div>
        <h3 class="sub_head">Search <br>Results.</h3>
        {{#each results}}
        <div class="search_bar">
        <div class="result_box">        
          <div class="row">
            <div class="col-md-12">
              <div class="col-md-2 col-sm-6 col-xs-12">
                <div class="pkg_img_cnt_square">

                </div>
              </div>
              <div class="col-md-3 col-sm-6 col-xs-12">
               <h4>TIME & DATE</h4>
               <p>{{this.time}} {{this.date}}</p>
              </div>
              <div class="col-md-3 col-sm-6 col-xs-12">
               
                  <h4>{{this.traveltime}}</h4>
                  <p>{{this.departure}}-{{this.arrival}}</p>
                
              </div>
               <div class="col-md-2 col-sm-6 col-xs-12"> 
               
                  <h4>{{this.rate}}</h4>
                  <p>{{this.roundtrip}}</p>
                
               </div> 
               <div class="col-md-2 col-sm-6 col-xs-12">
                   <button type="submit" class="btn btn-md btn_bg_black">Book Flight</button> 
               </div>
            </div>
          </div>
        </div>
        </div>
       {{/each}}
